# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SupChats' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SupChats

  target 'SupChatsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SupChatsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

pod 'RongCloudIMKit'
pod 'LeanCloud'			# LeanCloud swift Server SDK
pod 'ChatKit'			# Chats Message model	
pod 'AVOSCloud'			# 数据存储、短信、云引擎调用等基础服务模块 OC SDK
pod 'AVOSCloudIM'             	# 即时通讯模块 OC SDK
pod 'swiftScan'			# Scan and Creat BarCode
pod 'LeanCloudFeedback'		# LeanCloud Feedback
pod 'Alamofire'			# Alamofire is an HTTP networking library
pod 'SnapKit'			# SnapKit is a DSL to make Auto Layout
pod 'Kingfisher'		# Downloading and caching images from the web

end
